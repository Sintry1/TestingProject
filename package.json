{
  "name": "omnihost-systems",
  "version": "1.0.0",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js",
    "start": "nx run-many --target=serve --projects=api,frontend",
    "start:api": "nx serve api",
    "start:frontend": "nx serve frontend",
    "build": "nx build",
    "build:api": "nx build api",
    "build:omnihost": "nx build frontend",
    "test": "nx run-many --all --target=test",
    "test:ci": "nx run-many --all --target=test --skip-nx-cache",
    "e2e": "npm run e2e:open",
    "e2e:open": "npm run e2e:frontend:open",
    "e2e:frontend:open": "nx e2e frontend-e2e --watch --browser=chrome",
    "e2e:ci": "nx run-many --target=e2e --all --maxParallel=5 --browser chrome",
    "lint": "nx workspace-lint && nx run-many --target=lint --all=true",
    "format:write": "nx format:write --base=development",
    "format:check": "nx format:check --base=development",
    "seed": "nx run seeder:build && node ./dist/apps/seeder/main"
  },
  "private": true,
